{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "barryvdh/laravel-snappy": "^0.4.1",
        "doctrine/dbal": "^2.6",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "laravelnews/laravel-twbs4": "^1.1",
        "maatwebsite/excel": "~2.1.0",
        "milon/barcode": "^5.3",
        "spatie/laravel-activitylog": "^2.4",
        "spatie/laravel-backup": "^5.1",
        "spatie/laravel-medialibrary": "6.0.0",
        "swiftmailer/swiftmailer": "^6.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "require-dev": {
        "filp/whoops":"~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
         "files" : [
           "app/helpers.php"
         ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Biblioteca\\" : "app/Archivio/Biblioteca",
            "App\\Authentication\\" : "app/Archivio/Auth",
            "App\\Admin\\" : "app/Archivio/Admin",
            "App\\Core\\" : "app/Archivio/Core",
            "App\\Officina\\" : "app/Archivio/Officina",
            "App\\Nomadelfia\\" : "app/Archivio/Nomadelfia",
            "App\\Anagrafe\\" : "app/Archivio/Anagrafe",
            "App\\Patente\\" : "app/Archivio/Patente"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
           "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
           "@php artisan package:discover"
       ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
