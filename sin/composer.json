{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-dompdf": "^0.8",
        "barryvdh/laravel-snappy": "^0.4",
        "doctrine/dbal": "^2.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "~2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.9",
        "phpoffice/phpspreadsheet": "^1.7",
        "phpoffice/phpword": "^0.17.0",
        "pmatseykanets/laravel-sql-migrations": "^1.0.0",
        "spatie/laravel-activitylog": "^3.0",
        "spatie/laravel-backup": "^6.0",
        "spatie/laravel-medialibrary": "^9.0.0",
       "spatie/laravel-medialibrary-pro": "^1.0.0",
        "wemersonjanuario/wkhtmltopdf-windows": "0.12.2.3"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Factories\\": "database/factories/",
            "App\\Biblioteca\\": "app/Archivio/Biblioteca",
            "App\\Auth\\": "app/Archivio/Auth",
            "App\\Admin\\": "app/Archivio/Admin",
            "App\\Core\\": "app/Archivio/Core",
            "App\\Officina\\": "app/Archivio/Officina",
            "App\\Nomadelfia\\": "app/Archivio/Nomadelfia",
            "App\\Anagrafe\\": "app/Archivio/Anagrafe",
            "App\\Scuola\\": "app/Archivio/Scuola",
            "App\\Rtn\\": "app/Archivio/Rtn",
            "App\\Patente\\": "app/Archivio/Patente",
            "App\\ArchivioDocumenti\\": "app/Archivio/ArchivioDocumenti"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ]
}
